# API base for same-origin backend endpoints
VITE_API_BASE=/api

# Default chunk size for uploads (bytes)
VITE_DEFAULT_CHUNK_BYTES=32768

# Upper bounds for chunks and files (bytes)
VITE_MAX_CHUNK_BYTES=262144
VITE_MAX_FILE_BYTES=524288000

# Origin hint for the app (optional)
VITE_APP_ORIGIN=http://localhost:5173
